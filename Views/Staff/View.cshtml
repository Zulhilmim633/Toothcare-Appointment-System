@model Toothcare_Appointment_System.Models.Staff

<h2>User page</h2>

<div>
    <dl class="row">
        <dt class="col-sm-2">
            Staff Name
        </dt>
        <dd class="col-sm-10">
            @Model.StaffName
        </dd>
        <dt class="col-sm-2">
            Phone Number
        </dt>
        <dd class="col-sm-10">
            @Model.StaffPhoneNo
        </dd>
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Model.StaffEmail
        </dd>
        <dt class="col-sm-2">
            Role
        </dt>
        <dd class="col-sm-10">
            @Model.StaffRole
        </dd>
    </dl>

    <a asp-action="Index" class="btn btn-secondary">Back</a>
</div>
