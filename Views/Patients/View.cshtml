@model Toothcare_Appointment_System.Models.Patients

<h2>Patient</h2>

<div>
	<dl class="row">
		<dt class="col-sm-2">
			Patient Name
		</dt>
		<dd class="col-sm-10">
			@Model.PatientName
		</dd>
		<dt class="col-sm-2">
			Phone Number
		</dt>
		<dd class="col-sm-10">
			@Model.PatientPhoneNo
		</dd>
		<dt class="col-sm-2">
			Email
		</dt>
		<dd class="col-sm-10">
			@Model.PatientEmail
		</dd>
		<dt class="col-sm-2">
			Date of Birth
		</dt>
		<dd class="col-sm-10">
			@Model.PatientDOB
		</dd>
		<dt class="col-sm-2">
			Address
		</dt>
		<dd class="col-sm-10">
			@Model.PatientAddress
		</dd>
		<dt class="col-sm-2">
			Gender
		</dt>
		<dd class="col-sm-10">
			@Model.PatientGender
		</dd>
	</dl>

	<a asp-action="Index" class="btn btn-secondary">Back</a>
</div>
